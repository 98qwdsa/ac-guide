  <form bindsubmit='submit'>
    <view class="content">
      <checkbox-group bindchange="checkboxChange">
        <label class="checkbox-item" wx:for="{{roles}}" wx:key="index">
          <checkbox value="{{item.code}}" checked="{{item.checked}}"/> {{item.code}}
        </label>
      </checkbox-group>
    </view>
    <view class='event_buttons'>
      <button form-type='reset' class="reset">取消</button>
      <button form-type='submit' class="submit" >确定</button>
    </view>
  </form>
